{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "package": "remix build && ncc build ./build/index.js",
    "dev": "remix dev",
    "start": "remix-serve build",
    "tidy": "xo --fix --plugin=react ./app"
  },
  "dependencies": {
    "@remix-run/node": "1.7.5",
    "@remix-run/react": "1.7.5",
    "@remix-run/serve": "1.7.5",
    "@stayradiated/preprocess-markdown": "workspace:*",
    "date-fns": "2.29.3",
    "markdown-to-jsx": "7.1.7",
    "photoswipe": "5.3.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tiny-invariant": "1.3.1",
    "validator": "13.7.0",
    "zod": "3.19.1"
  },
  "devDependencies": {
    "@remix-run/dev": "1.7.5",
    "@remix-run/eslint-config": "1.7.5",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "@types/validator": "13.7.10",
    "@vercel/ncc": "0.34.0",
    "eslint": "8.27.0",
    "typescript": "4.8.4",
    "xo": "0.52.4"
  },
  "engines": {
    "node": ">=14"
  },
  "xo": {
    "space": true,
    "semicolon": false,
    "prettier": true,
    "rules": {
      "import/extensions": "off",
      "@typescript-eslint/naming-convention": "off",
      "n/file-extension-in-import": "off"
    }
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "all",
    "useTabs": false
  }
}
